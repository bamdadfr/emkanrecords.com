{
  "name": "emkanrecords-www",
  "version": "1.4.17",
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "eject": "react-scripts eject",
    "lint": "yarn lint:es && yarn lint:style",
    "lint:es": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:style": "stylelint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "package:clean": "rm -rf node_modules",
    "package:reinstall": "yarn package:clean && yarn",
    "package:sort": "npx sort-package-json",
    "start": "BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.20"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^1.3.29",
    "@bamdadsabbagh/stylelint-config": "^1.2.43",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "eslint-plugin-prettier": "^3.1.4",
    "react-scripts": "^4.0.0",
    "typescript": "^4.0.3"
  }
}
