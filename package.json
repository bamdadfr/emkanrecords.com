{
    "name": "emkanrecords-www",
    "version": "1.13.18",
    "description": "Emkan Records - digital record label from France.",
    "license": "MIT",
    "author": {
        "name": "Bamdad Sabbagh",
        "email": "hello@bamdadsabbagh.com",
        "url": "https://bamdadsabbagh.com/"
    },
    "scripts": {
        "build": "next build && yarn build:post",
        "build:post": "find .next -type f -name \"*.map\" -exec rm {} \\;",
        "clean": "yarn next:clean && yarn package:clean",
        "dev": "yarn next:clean && next dev",
        "export": "next export",
        "lint": "yarn lint:es",
        "lint:es": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
        "lint:style": "yarn lint:style:components && yarn lint:style:pages",
        "lint:style:components": "stylelint 'src/components/**/*.{css,scss,js,jsx,ts,tsx}' --fix",
        "lint:style:pages": "stylelint 'src/pages/**/*.{css,scss,js,jsx,ts,tsx}' --fix",
        "next:clean": "rimraf .next",
        "package:clean": "rimraf node_modules",
        "package:reinstall": "yarn clean && yarn",
        "package:sort": "npx sort-package-json",
        "start": "next start",
        "test": "jest",
        "test:cov": "jest --coverage && codecov",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@react-spring/web": "^9.2.4",
        "@sentry/nextjs": "^6.6.0",
        "next": "11.0.1",
        "prop-types": "^15.7.2",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@bamdadsabbagh/eslint-config": "^1.14.0",
        "@bamdadsabbagh/stylelint-config": "^1.3.18",
        "@fontsource/roboto": "^4.5.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.1",
        "@types/react": "^17.0.14",
        "@types/react-dom": "^17.0.9",
        "babel-jest": "^27.0.2",
        "codecov": "^3.8.2",
        "eslint-config-next": "^11.0.1",
        "jest": "^27.0.4",
        "jest-css-modules": "^2.1.0",
        "jest-transform-stub": "^2.0.0",
        "react-icons": "^4.2.0",
        "rimraf": "^3.0.2",
        "sass": "^1.37.4",
        "sass-reset": "^1.0.2"
    }
}
